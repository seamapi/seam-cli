{
  "name": "seam-cli",
  "module": "index.ts",
  "type": "module",
  "version": "0.0.7",
  "repository": "git@github.com:seamapi/seam-cli.git",
  "scripts": {
    "download-openapi": "curl https://connect.getseam.com/openapi.json -o ./lib/openapi.json",
    "cli": "tsx ./cli.ts",
    "build": "tsup",
    "build:binaries": "pkg --out-path bin ./dist/cli.cjs",
    "build:binaries:mac": "pkg --out-path bin -t node18-macos-arm64 ./dist/cli.cjs"
  },
  "bin": {
    "seam": "./dist/cli.cjs"
  },
  "tsup": {
    "entry": [
      "cli.ts"
    ],
    "format": "cjs",
    "sourcemap": true
  },
  "devDependencies": {
    "@swc/core": "^1.3.100",
    "@types/configstore": "^6.0.2",
    "@types/lodash": "^4.14.202",
    "@types/minimist": "^1.2.5",
    "@types/openapi-v3": "^3.0.0",
    "@types/prompts": "^2.4.9",
    "bun-types": "latest",
    "chalk": "^5.3.0",
    "configstore": "^6.0.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "pkg": "^5.8.1",
    "prompts": "^2.4.2",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@seamapi/http": "^0.10.0",
    "redaxios": "^0.5.1",
    "swagger-parser": "^10.0.3"
  }
}
